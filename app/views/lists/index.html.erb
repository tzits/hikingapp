<div class='container'>
  <% @lists.each do |l| %>
    <div class="col-sm-12 col-lg-4" style='float:left'><h1><%= link_to l.name, category_path(l,l.categories.first)%></h1>
      <br>
      <%= image_tag l.image, class: 'col-sm-12 col-lg-12'%>
      <% if current_user && current_user.admin == true %>
      <%= link_to 'Edit List' edit_list_path(l) %>
      <% end %>
    </div>
  <% end %>
</div>
